        "inputAnchors": [],
        "inputs": {
          "customFunctionInputVariables": [
            {
              "variableName": "base64",
              "variableValue": "<p><span class=\"variable\" data-type=\"mention\" data-id=\"question\" data-label=\"question\">{{ question }}</span> </p>"
            }
          ],
          "customFunctionJavascriptFunction": "/*\n * Extracts Base64 audio from a combined input like:\n *   \"base64:xxxxxxx Phone: +91xxxx\"\n * Uploads it to tmpfiles.org and returns a public download URL for Twilio <Play>.\n */\n\nconst fetch = require('node-fetch');\n\n// 1️⃣ Extract base64 from the input\nlet input = $base64; // from the input variable in Flowise\nif (typeof input !== 'string') throw new Error('Invalid input');\n\nlet base64Match = input.match(/base64:([\\s\\S]*?)\\s*Phone:/);\nif (!base64Match || !base64Match[1]) throw new Error('No base64 audio found');\nlet b64 = base64Match[1].trim();\n\n// 2️⃣ Decode Base64 to buffer\nconst buffer = Buffer.from(b64, 'base64');\n\n// 3️⃣ Upload the WAV buffer to tmpfiles.org\nconst boundary = '----tmpBoundary' + Date.now();\nlet head = '';\nhead += `--${boundary}\\r\\n`;\nhead += `Content-Disposition: form-data; name=\"file\"; filename=\"tts.wav\"\\r\\n`;\nhead += `Content-Type: audio/wav\\r\\n\\r\\n`;\nconst tail = `\\r\\n--${boundary}--\\r\\n`;\nconst body = Buffer.concat([\n  Buffer.from(head, 'utf8'),\n  buffer,\n  Buffer.from(tail, 'utf8'),\n]);\n\nconst res = await fetch('https://tmpfiles.org/api/v1/upload', {\n  method: 'POST',\n  headers: { 'Content-Type': `multipart/form-data; boundary=${boundary}` },\n  body,\n});\n\nconst text = await res.text();\nlet json;\ntry {\n  json = JSON.parse(text);\n} catch {\n  throw new Error('Invalid JSON from tmpfiles.org: ' + text.slice(0, 200));\n}\n\nif (!json?.data?.url) throw new Error('Upload failed: ' + text);\nconst fileUrl = json.data.url.replace('tmpfiles.org/', 'tmpfiles.org/dl/');\n\n// 4️⃣ Return the public direct link (usable by Twilio <Play>)\nreturn fileUrl;\n",
          "customFunctionUpdateState": "",
          "undefined": ""
        },
